    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
